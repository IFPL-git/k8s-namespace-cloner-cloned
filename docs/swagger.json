{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "Kubernetes Namespace Cloner API URI:\u003cbr\u003e\u0026nbsp;\u0026nbsp;https://{nw-server-hostname}:8080/api/v1\u003cbr\u003e\u003cbr\u003e",
        "title": "Kubernetes Namespace Cloner API",
        "contact": {},
